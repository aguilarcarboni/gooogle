@page "/about"
@rendermode InteractiveServer

<PageTitle>About the Project</PageTitle>

<style>
    body {
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        margin: 0;
        padding: 0;
        background-color: #f4f4f4;
    }

    .container {
        max-width: 800px;
        margin: 0 auto;
        background-color: #fff;
        padding: 20px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        border-radius: 10px;
        margin-top: 30px;
    }

    h1 {
        color: #333;
        text-align: center;
        margin-top: 20px;
        font-weight: 300;
    }

    p {
        color: #555;
        font-size: 16px;
        line-height: 1.6;
        margin: 15px 0;
    }

    .developers {
        margin-top: 30px;
    }

    .developers h2 {
        color: #333;
        font-weight: 300;
    }

    .developers ul {
        list-style: none;
        padding: 0;
    }

    .developers ul li {
        margin: 10px 0;
        font-size: 16px;
        color: #555;
    }

    .developers ul li span {
        font-weight: bold;
    }

</style>

<div class="container">
    <h1>Google sucks.</h1>
    <p>- 2024</p>
</div>

